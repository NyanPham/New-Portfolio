r\n    overflow: hidden;\r\n    background-image: url('../images/shapes/3.jpg');\r\n\r\n    .header {\r\n        position: absolute;\r\n        top: 1rem;\r\n        left: 50%;\r\n        transform: translateX(-50%);\r\n        z-index: 2;\r\n        \r\n        &__wrap {\r\n            padding: 10px;\r\n            display:flex;\r\n            align-items: center;\r\n            justify-content: space-between;\r\n            width: 60vw;\r\n        }\r\n        &__left {\r\n            display: flex;\r\n            align-items: center;\r\n            gap: 2rem;\r\n\r\n            & li {\r\n                font-size: 18px;\r\n                font-weight: 700;\r\n            }\r\n        }\r\n\r\n        @media (max-width: 1200px) {\r\n            &__wrap {\r\n                width: 100vw;\r\n            }\r\n        }\r\n\r\n        @media (max-width: 650px) {\r\n            &__sm {\r\n                display: none;\r\n            }\r\n        }\r\n\r\n        @media (max-width: 370px) {\r\n            & {\r\n                left: 36%;\r\n            }\r\n            \r\n            &__wrap {\r\n                gap: 20%;\r\n            }\r\n\r\n            .coolLink[data-text=\"Home\"] {\r\n                display: none;\r\n            }\r\n        }\r\n    }\r\n\r\n    .socials {\r\n        position: absolute;\r\n        left: 50%;\r\n        transform: translateX(-50%);\r\n        bottom: 10rem;\r\n        display: flex;\r\n        align-items: center;\r\n        gap: 1rem;\r\n        z-index: 2;\r\n    }\r\n}\r\n\r\n.scrollDown {\r\n    position: absolute;\r\n    bottom: 5rem;\r\n    left: 50%;\r\n    transform: translateX(-50%);\r\n    width: 50px;\r\n    height: 50px;\r\n\r\n    &__wheel {\r\n        height: 42px;\r\n        width: 24px;\r\n        border-radius: 10px;\r\n        border: 2px solid #fff;\r\n        margin-left: auto;\r\n        margin-right: auto;\r\n        position: relative;\r\n\r\n        &--inner {\r\n            position: absolute;\r\n            left: 50%;\r\n            transform: translateX(-50%);\r\n            margin-top: 5px;\r\n            margin-left: auto;\r\n            margin-right: auto;\r\n            width: 4px;\r\n            height: 4px;\r\n            background: #fff;\r\n            border-radius: 50%;\r\n            animation: movingwheel 0.5s linear infinite;\r\n\r\n            @keyframes movingwheel {\r\n                0% {\r\n                    top: 1px;\r\n                }\r\n\r\n                25% {\r\n                    top: 2px;\r\n                }\r\n\r\n                50% {\r\n                    top: 3px;\r\n                }\r\n\r\n                75% {\r\n                    top: 2px;\r\n                }\r\n\r\n                100% {\r\n                    top: 1px;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    &__arrows {\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n        gap: 0.5rem;\r\n\r\n        .scrollDown__arrow {\r\n            width: 15px;\r\n            height: 15px;\r\n            border-right: 2px solid #fff;\r\n            border-bottom: 2px solid #fff;\r\n            transform: rotate(45deg);\r\n            animation: scrollAnim 1s infinite;\r\n\r\n            &:nth-child(1) {\r\n                animation-delay: 0.1s;\r\n            }\r\n\r\n            &:nth-child(2) {\r\n                animation-delay: 0.2s;\r\n            }\r\n\r\n            &:nth-child(3) {\r\n                animation-delay: 0.3s;\r\n            }\r\n\r\n            @keyframes scrollAnim {\r\n                0% {\r\n                    opacity: 0;\r\n                }\r\n\r\n                50% {\r\n                    opacity: 0.5;\r\n                }\r\n\r\n                100% {\r\n                    opacity: 1;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.intro {\r\n    position: absolute;\r\n    top: 20%;\r\n    left: 1rem;\r\n    width: 100%;\r\n    height: 60%;\
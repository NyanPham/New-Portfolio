n        }\r\n\r\n        &-subtitle {\r\n            font-size: 27px;\r\n            font-weight: 700;\r\n        }\r\n    }\r\n}","assets\\styles\\reviews.scss":"@import \"./base.scss\";\r\n.reviews {\r\n    position: relative;\r\n    background: $darkish-blue;\r\n    color: #fff;\r\n    display: none;\r\n\r\n    &__header {\r\n        display: flex;\r\n        align-items: center;\r\n        gap: 1rem;\r\n        .me {\r\n            position: relative;\r\n            background: #28316013;\r\n            box-shadow: 0 8px 32px 0 $box-shadow-blue1;\r\n\r\n            &__curve {\r\n                position: absolute;\r\n                top: -10px;\r\n                left: 20px;\r\n                width: 30%;\r\n                height: 120%;\r\n                border-left: 2px solid $red-color;\r\n                border-radius: 50%;\r\n                transform: rotate(-20deg);\r\n                box-shadow: 0 8px 32px 0 $box-shadow-blue2;\r\n            }\r\n\r\n            &__infos {\r\n                display: flex;\r\n                align-items: center;\r\n                gap: 15px;\r\n                font-size: 17px;\r\n                font-family: sans-serif;\r\n            }\r\n                \r\n            &__img {\r\n                width: 150px;\r\n                height: 150px;\r\n                border-radius: 50%;\r\n                border-right: 2px solid $red-color;\r\n                object-fit: cover;\r\n            }\r\n\r\n            &__col {\r\n                display: flex;\r\n                flex-direction: column;\r\n                gap: 5px;\r\n                padding-right: 1.2rem;\r\n            }\r\n\r\n            &__info {\r\n                display: flex;\r\n                align-items: center;\r\n                gap: 10px;\r\n\r\n                & svg {\r\n                    width: 20px;\r\n                    height: 20px;\r\n                    fill: #fff;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.swiper__wrapper {\r\n    padding: 2rem 0;\r\n}\r\n\r\n.swiper {\r\n    min-height: 500px;\r\n}\r\n\r\n.review {\r\n    position: relative;\r\n    display: flex;\r\n    font-size: 16px;\r\n    padding: 5px;\r\n\r\n    &__card {\r\n        position: relative;\r\n        width: 100%;\r\n        padding: 2rem 0;\r\n        display: flex;\r\n\r\n        @media (max-width: 650px) {\r\n            & {\r\n                font-size: 18px;\r\n                font-family: sans-serif;\r\n            }\r\n        }\r\n    }\r\n\r\n    & svg {\r\n        width: 30px;\r\n        fill: $red-color;\r\n        height: max-content;\r\n\r\n        @media (min-width: 500px) {\r\n            & {\r\n                width: 40px;\r\n            }\r\n        }\r\n    }\r\n    \r\n    &__topborder {\r\n        width: 100%;\r\n        height: 1px;\r\n        position: absolute;\r\n        top: 0;\r\n        left: 0;\r\n        background: #fff;\r\n    }\r\n\r\n    &__text {\r\n        user-select: none;\r\n        span {\r\n            font-family: sans-serif;\r\n            font-size: 30px;\r\n            font-weight: 600;\r\n            color: $red-color;\r\n        }\r\n    }\r\n\r\n    &__img {\r\n        width: 100px;\r\n        height: 100px;\r\n        object-fit: cover;\r\n        border-radius: 35px;\r\n        \r\n        @media (min-width: 400px) {\r\n            & {\r\n                width: 150px;\r\n                height: 150px;\r\n            }\r\n        }\r\n    }\r\n\r\n    &__profile {\r\n        position: absolute;\r\n        left: 5rem;\r\n        bottom: -2rem;\r\n        display: flex;\r\n        flex-direction: column;\r\n        font-family: serif;\r\n        font-size: 17px;\r\n    }\r\n}\r\n\r\n.swiper-slide {\r\n    filter: blur(2px) !important;\r\n    transform: scale(0.8);\r\n    transition: all 0.2s ease;\r\n}\r\n\r\n\r\n.swiper-slide-active {\r\n    filter: blur(0) !important;\r\n    transform: scale(1);\r\n}\r\n\r\n.swiper__tools {\r\n    position: relative;\